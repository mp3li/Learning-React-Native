,[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/hello-wave.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/master-detail.tsx",["99","100"],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/parallax-scroll-view.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/responsive-layout.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/themed-text.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/themed-view.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/ui/collapsible.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/ui/icon-symbol.ios.tsx",[],[],"/Users/stellar/School/SOFT210 - React Native/Learning React Native/learning-react-native-app/components/ui/icon-symbol.tsx",[],[],{"ruleId":"101","severity":1,"message":"102","line":6,"column":3,"nodeType":null,"messageId":"103","endLine":6,"endColumn":20},{"ruleId":"101","severity":1,"message":"104","line":39,"column":9,"nodeType":null,"messageId":"103","endLine":39,"endColumn":18},{"ruleId":"101","severity":1,"message":"105","line":31,"column":65,"nodeType":null,"messageId":"103","endLine":31,"endColumn":76},{"ruleId":"106","severity":1,"message":"107","line":145,"column":1,"nodeType":"108","endLine":145,"endColumn":44,"fix":"109"},"@typescript-eslint/no-unused-vars","'AccessibilityRole' is defined but never used.","unusedVar","'textColor' is assigned a value but never used.","'screenWidth' is assigned a value but never used.","import/first","Import in body of module; reorder to top.","ImportDeclaration",{"range":"110","text":"111"},[0,4565],"import React, { useState } from 'react';\nimport { View, StyleSheet, ScrollView, FlatList } from 'react-native';\nimport { useResponsiveLayout } from '@/hooks/use-responsive-layout';\nimport { ThemedView } from './themed-view';\n\n// Import ThemedText and ThemedView from existing components\nimport { ThemedText } from './themed-text';\n\nexport interface ListItem {\n  id: string;\n  title: string;\n  description?: string;\n  icon?: React.ReactNode;\n}\n\ninterface MasterDetailComponentProps {\n  items: ListItem[];\n  renderDetail: (item: ListItem) => React.ReactNode;\n  renderListItem?: (item: ListItem, isSelected: boolean) => React.ReactNode;\n  headerComponent?: React.ReactNode;\n}\n\n/**\n * Master-Detail layout component that shows a list on master side\n * and detailed content on detail side on tablets, single view on phones\n */\nexport function MasterDetailComponent({\n  items,\n  renderDetail,\n  renderListItem,\n  headerComponent,\n}: MasterDetailComponentProps) {\n  const [selectedItem, setSelectedItem] = useState<ListItem>(items[0] || null);\n  const { isTablet, masterDetailColumnWidth, detailColumnWidth, screenWidth } =\n    useResponsiveLayout();\n\n  if (!selectedItem && items.length > 0) {\n    setSelectedItem(items[0]);\n  }\n\n  const defaultListItem = (item: ListItem) => (\n    <ThemedView\n      style={[\n        styles.listItem,\n        selectedItem?.id === item.id && styles.selectedListItem,\n      ]}>\n      {item.icon && <View style={styles.itemIcon}>{item.icon}</View>}\n      <View style={styles.itemContent}>\n        <ThemedText\n          type=\"defaultSemiBold\"\n          numberOfLines={1}\n          accessibilityRole=\"menuitem\"\n          accessibilityLabel={item.title}\n          accessibilityState={{\n            selected: selectedItem?.id === item.id,\n          }}>\n          {item.title}\n        </ThemedText>\n        {item.description && (\n          <ThemedText\n            style={styles.itemDescription}\n            numberOfLines={1}>\n            {item.description}\n          </ThemedText>\n        )}\n      </View>\n    </ThemedView>\n  );\n\n  // On phone, show either list or detail\n  if (!isTablet) {\n    return (\n      <ScrollView style={styles.phoneContainer}>\n        {headerComponent && <View style={styles.headerComponent}>{headerComponent}</View>}\n        <FlatList\n          scrollEnabled={false}\n          data={items}\n          keyExtractor={(item) => item.id}\n          renderItem={({ item }) => (\n            <ThemedView\n              style={styles.phoneListItem}\n              accessible={true}\n              accessibilityRole=\"button\"\n              accessibilityLabel={item.title}\n              onTouchEnd={() => setSelectedItem(item)}>\n              {renderListItem ? renderListItem(item, selectedItem?.id === item.id) : defaultListItem(item)}\n            </ThemedView>\n          )}\n        />\n        {selectedItem && (\n          <View style={styles.phoneDetailSection}>\n            {renderDetail(selectedItem)}\n          </View>\n        )}\n      </ScrollView>\n    );\n  }\n\n  // On tablet, show master-detail side by side\n  return (\n    <View style={styles.tabletContainer}>\n      {/* Master column */}\n      <View style={[styles.masterColumn, { width: masterDetailColumnWidth }]}>\n        {headerComponent && <View style={styles.headerComponent}>{headerComponent}</View>}\n        <FlatList\n          data={items}\n          keyExtractor={(item) => item.id}\n          renderItem={({ item }) => (\n            <ThemedView\n              accessible={true}\n              accessibilityRole=\"button\"\n              accessibilityLabel={item.title}\n              accessibilityState={{\n                selected: selectedItem?.id === item.id,\n              }}\n              onTouchEnd={() => setSelectedItem(item)}\n              style={styles.masterListItemContainer}>\n              {renderListItem\n                ? renderListItem(item, selectedItem?.id === item.id)\n                : defaultListItem(item)}\n            </ThemedView>\n          )}\n        />\n      </View>\n\n      {/* Detail column */}\n      <View style={[styles.detailColumn, { width: detailColumnWidth }]}>\n        {selectedItem ? (\n          <ScrollView style={styles.detailContent}>\n            {renderDetail(selectedItem)}\n          </ScrollView>\n        ) : (\n          <View style={styles.emptyState}>\n            <ThemedText\n              type=\"subtitle\"\n              accessibilityRole=\"header\"\n              accessibilityLabel=\"No item selected\">\n              Select an item to view details\n            </ThemedText>\n          </View>\n        )}\n      </View>\n    </View>\n  );\n}"]